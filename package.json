{
  "name": "skylark-cropperjs",
  "description": "A version of cropperjs that ported to running on skylarkjs.",
  "version": "0.9.0",
  "keywords": [
    "skylark",
    "framework",
    "vue"
  ],
  "homepage": "https://github.com/skylark-integration/skylark-cropperjs/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-integration/skylark-cropperjs.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-integration/skylark-cropperjs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-langx": "git+https://github.com/skylarklangx/skylark-langx.git#master"
  },
  "skylarkjs": {
    "build": {
      "prepare": {
        "texttojs": [
          "css",
          "html"
        ],
        "jsxtojs": false,
        "es6toamd": true,
        "cjstoamd": false
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
